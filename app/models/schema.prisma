generator client {
  provider = "prisma-client-js"
  output   = "../../lib/prisma-client"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Product {
  id        Int     @id @default(autoincrement())
  code      String  @unique
  title     String
  label_title     String
}